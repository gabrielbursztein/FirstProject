<%= form_for :book, url: books_path do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br>
  <%= f.label :author %>
  <%= f.text_field :author %> <br>
  <%= f.label :year %>
  <%= f.number_field :year %> <br>
  <%= f.label :description %>
  <%= f.text_field :description %> <br>
  <%= f.label :image %>
  <%= f.file_field :image %> <br>
  <%= f.submit %>
<% end %>
